<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@drawable/search_x_magnifier">

    <target
        android:animation="@animator/magnifier_to_line"
        android:name="magnifier_path" />

    <target
        android:animation="@animator/magnifier_to_cross"
        android:name="magnifier" />

    <target
        android:animation="@animator/line_to_cross_path"
        android:name="line_path" />

    <target
        android:animation="@animator/line_to_cross"
        android:name="line" />

</animated-vector>
